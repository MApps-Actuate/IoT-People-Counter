//var containerHeight = $("#" + containerId).height();

var startDate = new Date();

function dataTransfrom(cursor) {
	var stats = {};
	while (cursor.hasNext()) {
		var rowData = cursor.next();
		//var ctr = 0;
		var epochTimestamp = Math.round(+new Date(startDate.getFullYear(), startDate.getMonth(), startDate.getDate(), rowData["theEventTime"], 0, 0)/1000);
		stats[String(epochTimestamp)] = rowData["totalpeoplecount"];
		//ctr++;
	}
	return stats;
}

myData2 = dataTransfrom(customViz.getDataCursor());


var callback = function() {

	var cal = new CalHeatMap();
	//var thisContainerID = "#" + containerId;
	var thisContainerID = "#example-h";
	//cal.init({});
	
	cal.init({
subDomain: "hour",
subDomainTextFormat: function(date,value) {
debugger;
		return value;
	},
		data: myData2,
		start: startDate,
		itemSelector: thisContainerID,
		domainLabelFormat: "OT 9000-2",
		domain: "day",
		range: 1,
		domainMargin: 10,
		colLimit: 24,
		displayLegend: false,
		cellSize: 25,
		cellRadius: 3,
		cellPadding: 5,
		label: {
			position: "left",
			offset: { 	x: 10, y: 20 }
		}
	});
}

