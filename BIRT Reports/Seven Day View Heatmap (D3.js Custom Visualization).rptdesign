            <xml-property name="queryText"><![CDATA[SELECT SUM(peoplecount) as totalpeoplecount,
	YEAR(event_time) as theYearEventTime,
	MONTH(event_time) as theMonthEventTime,
	DAY(event_time) as theDayEventTime,
	HOUR(event_time) as theHourEventTime,
	node
FROM iot_people_counter
WHERE iot_people_counter.event_time BETWEEN DATE_ADD(CURDATE(), INTERVAL -7 day) AND DATE_ADD(CURDATE(), INTERVAL 1 day)
GROUP BY YEAR(event_time), MONTH(event_time), DAY(event_time), HOUR(event_time), node]]></xml-property>
