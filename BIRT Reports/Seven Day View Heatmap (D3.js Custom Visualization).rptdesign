                        <method name="clientOnCreate"><![CDATA[// Cal-heatmap
// http://kamisama.github.io/cal-heatmap/
//
var customViz = this;
//debugger;
var containerId = this.getId();
//var containerWidth = $("#" + containerId).width();
//var containerHeight = $("#" + containerId).height();

var startDate = new Date();
startDate.setDate(startDate.getDate() - 6);


function dataTransfrom(cursor) {
	var stats = {};
	while (cursor.hasNext()) {
		var rowData = cursor.next();
		var epochTimestamp = Math.round(+new Date( rowData["theYearEventTime"],( rowData["theMonthEventTime"]-1),  rowData["theDayEventTime"], rowData["theHourEventTime"], 0, 0)/1000);
		stats[String(epochTimestamp)] = rowData["totalpeoplecount"];
	}
	return stats;	
}

myData = dataTransfrom(customViz.getDataCursor());


var callback = function() {

	var cal = new CalHeatMap();
	//var thisContainerID = "#" + containerId;
	var thisContainerID = "#example-h";
	//cal.init({});
	
	cal.init({

		data: myData,
		start: startDate,
		itemSelector: thisContainerID,
		domain: "day",
		range: 7,
		displayLegend: false,
		domainGutter: 10,
		domainLabelFormat:  "%m-%d",
		cellSize: 20,
		label: {
			position: "top",
		},
		
		onClick: function(date, nb) {
			window.location.href = "/iportal/PeopleCounterHourlyJSAPI.jsp?theDate=" +  date.toISOString().slice(0,10);
		}
		
	});
}

head.load('ActivityTracker/d3.v3.min.js','ActivityTracker/cal-heatmap.min.js',callback);]]></method>
